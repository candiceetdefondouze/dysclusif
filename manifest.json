{
    "manifest_version": 2,
    "name": "Dysclusif",
    "description": "Removing inclusive writing for dyslexic people",
    "version": "1.2.2",
    "author": "Candiceetdefondouze",
    "browser_specific_settings": {
        "gecko": {
            "id": "candiceetdefondouze@gmail.com",
            "strict_min_version": "78.0"
        }
    },
    "browser_action": {
        "default_popup": "mainPopup/popup.html",
        "default_title": "Dysclusif",
        "default_icon": "images/internet-32px.png"
    },
    "icons": {
        "64": "images/internet.png",
        "32": "images/internet-32px.png",
        "16": "images/internet-16px.png"
    },
    "permissions": [
        "messagesModify",
        "storage"
    ],
    "background": {
        "scripts": [
            "background.js"
        ]
    }
}